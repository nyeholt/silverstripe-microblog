{
  "name": "symbiote-webpack-starter",
  "description": "Symbiote webpack starter project",
  "private": true,
  "author": "Symbiote",
  "version": "0.0.1",
  "repository": "",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "watch": "node_modules/.bin/webpack --watch --config webpack.dev.js",
    "start": "node_modules/.bin/webpack --watch --config webpack.dev.js",
    "devserver": "node_modules/.bin/webpack-dev-server --watch --config webpack.dev-server.js",
    "build": "node_modules/.bin/webpack --display-error-details --config webpack.prod.js",
    "storybook": "node_modules/.bin/start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
